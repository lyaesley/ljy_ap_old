CREATE TABLE "TSPADM"."PH_COMP_PROC_CMPT" 
   (	"CLAIM_ID" VARCHAR2(12) NOT NULL ENABLE, 
	"SVC_MGMT_NUM" VARCHAR2(10) NOT NULL ENABLE, 
	"PROC_DT" DATE NOT NULL ENABLE, 
	"COMP_TER_MDL" VARCHAR2(15), 
	"COMP_MFACT_CD" VARCHAR2(10), 
	"COMP_MFACT_NM" VARCHAR2(40), 
	"COMP_MOB_PRC" NUMBER, 
	"CUST_CHRG_AMT" NUMBER, 
	"AREA_CD" VARCHAR2(3), 
	"AREA_NM" VARCHAR2(20), 
	"AGEN_CD" VARCHAR2(10), 
	"AGEN_NM" VARCHAR2(20), 
	"STORE_ID" VARCHAR2(10), 
	"STORE_NM" VARCHAR2(20), 
	"CMPT_STS" VARCHAR2(10), 
	 CONSTRAINT "XPK_PH_COMP_PROC_CMPT_PK" PRIMARY KEY ("CLAIM_ID", "SVC_MGMT_NUM", "PROC_DT")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOLOGGING 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."CLAIM_ID" IS '클레임ID';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."SVC_MGMT_NUM" IS '서비스관리번호';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."PROC_DT" IS '처리일자';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."COMP_TER_MDL" IS '보상단말기종';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."COMP_MFACT_CD" IS '보상단말제조사코드';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."COMP_MFACT_NM" IS '보상단말제조사명';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."COMP_MOB_PRC" IS '보상단말기출고가';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."CUST_CHRG_AMT" IS '고객부담금';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."AREA_CD" IS '보상대리점구분코드';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."AREA_NM" IS '보상대리점구분명';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."AGEN_CD" IS '보상대리점코드';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."AGEN_NM" IS '보상대리점코드명';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."STORE_ID" IS '보상매장ID';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."STORE_NM" IS '보상매장명';
   COMMENT ON COLUMN "TSPADM"."PH_COMP_PROC_CMPT"."CMPT_STS" IS '보상완료코드';
   COMMENT ON TABLE "TSPADM"."PH_COMP_PROC_CMPT"  IS '보상처리완료';

