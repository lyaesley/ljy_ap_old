	"CLAIM_ID" NUMBER, 
	"MOB_PHN_NO" VARCHAR2(20), 
	"MAKING_NO" VARCHAR2(20), 
	"PROD_FG" VARCHAR2(5), 
	"MODEL_NM" VARCHAR2(20), 
