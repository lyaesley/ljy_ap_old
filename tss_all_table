	 CONSTRAINT "TS_CONT_SCLS_CD_PK1" PRIMARY KEY ("LCLS_CD", "SCLS_CD")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOLOGGING 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA"  ENABLE, 
	 CONSTRAINT "FK_CONT_SCLS_CD_01" FOREIGN KEY ("LCLS_CD")
	  REFERENCES "TSPADM"."TS_CONT_LCLS_CD" ("LCLS_CD") ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

   COMMENT ON COLUMN "TSPADM"."TS_CONT_SCLS_CD"."LCLS_CD" IS '대분류코드';
   COMMENT ON COLUMN "TSPADM"."TS_CONT_SCLS_CD"."SCLS_CD" IS '소분류코드';
   COMMENT ON COLUMN "TSPADM"."TS_CONT_SCLS_CD"."SCLS_CD_NM" IS '소분류코드명';
   COMMENT ON COLUMN "TSPADM"."TS_CONT_SCLS_CD"."SORT_ORD" IS '정렬순서';
   COMMENT ON COLUMN "TSPADM"."TS_CONT_SCLS_CD"."USE_YN" IS '사용여부';
   COMMENT ON COLUMN "TSPADM"."TS_CONT_SCLS_CD"."EXPL" IS '설명';
