   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."SEQ_NO" IS '고유번호';
   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."ACPTR_NM" IS '접수자명(입력값)';
   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."RSV_TEL_NO" IS '예약전화번호(입력값)';
   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."CUST_NM" IS '가입고객명';
   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."SVC_NUM" IS '가입휴대폰번호(분실휴대폰번호)';
   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."RES_CTR_ACPT_DY" IS '예약신청일';
   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."MEMO" IS '메모';
   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."SENT_YN_CD" IS '전송여부코드(Y/N)';
   COMMENT ON COLUMN "TSPADM"."TS_WE_RESERVE_INFO#"."IN_TYPE" IS '유입경로(W:WEB, A:ARS)';
   COMMENT ON TABLE "TSPADM"."TS_WE_RESERVE_INFO#"  IS '예약상담접수현황';

CREATE TABLE "TSPADM"."WEB_DOCU_ACCEPT" 
   (	"CLAIM_ID" VARCHAR2(12) NOT NULL ENABLE, 
	"DOCU_REG_DT" VARCHAR2(14) NOT NULL ENABLE, 
	"DOCU_CD" VARCHAR2(4) NOT NULL ENABLE, 
	"IMG_URL_INFO" VARCHAR2(80), 
	"READ_ST" VARCHAR2(1)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

CREATE TABLE "TSPADM"."WEB_REWARD_ACCEPT#" 
   (	"CLAIM_ID" VARCHAR2(12) NOT NULL ENABLE, 
	"SRC_NO" RAW(32) NOT NULL ENABLE, 
	"CTZ_CORP_NUM" VARCHAR2(13) NOT NULL ENABLE, 
	"SRC_MNG_NO" VARCHAR2(10) NOT NULL ENABLE, 
	"MOBL_SRL_NO" VARCHAR2(10), 
	"JOIN_DY" VARCHAR2(8) NOT NULL ENABLE, 
	"ACC_ST" VARCHAR2(1), 
	"ACC_DY" VARCHAR2(8), 
	"ACC_DT" VARCHAR2(2), 
	"ACC_PLC_CD" VARCHAR2(10), 
	"ACC_PLC" VARCHAR2(100), 
	"ACC_PROC" VARCHAR2(1000), 
	"REPORT_YN" VARCHAR2(1), 
	"REPORT_DY" VARCHAR2(8), 
	"RENT_YN" VARCHAR2(1), 
	"INSU_PRUD_CD" VARCHAR2(10), 
	"MOBL_MODL_CD" VARCHAR2(5), 
	"TEL_1" RAW(32), 
	"TEL_2" RAW(32), 
	"NOTI_AGR_YN" VARCHAR2(1), 
	"NOTI_AGR_ST" VARCHAR2(1), 
	"NOTI_AGR_EMAIL" RAW(64), 
	"NOTI_AGR_SMS" VARCHAR2(50), 
	"MEM_ST_CD" VARCHAR2(1), 
	"LOCAL_FORG_CD" VARCHAR2(1), 
	"REG_DT" VARCHAR2(14), 
	"READ_ST" VARCHAR2(1), 
	 CONSTRAINT "SYS_C0077220" PRIMARY KEY ("CLAIM_ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOLOGGING 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

