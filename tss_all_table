CREATE TABLE "TSPADM"."TSS_IT_DET" 
   (	"CD" VARCHAR2(20) NOT NULL ENABLE, 
	"CV1" VARCHAR2(40), 
	"CV2" VARCHAR2(40), 
	"CV3" VARCHAR2(40), 
	"CV4" VARCHAR2(40), 
	"CV5" VARCHAR2(40), 
	"CV6" VARCHAR2(40), 
	"DESC_ST" VARCHAR2(3000), 
	"DESC_IT" VARCHAR2(3000), 
	"EXP_DA" NUMBER, 
	"REG_DT" DATE NOT NULL ENABLE, 
	"UPD_DT" DATE NOT NULL ENABLE, 
	 CONSTRAINT "TSS_IT_DET_PK" PRIMARY KEY ("CD")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 NOLOGGING 
