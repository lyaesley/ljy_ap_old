CREATE TABLE "TSPADM"."PH_LOCATION_INFO" 
   (	"SGG_CD" VARCHAR2(5), 
	"BJ_D_CD" VARCHAR2(10) NOT NULL ENABLE, 
	"SD_NM" VARCHAR2(40), 
	"SGG_NM" VARCHAR2(40), 
	"EMD_NM" VARCHAR2(40), 
	"ROAD_CD" VARCHAR2(12) NOT NULL ENABLE, 
	"ROAD_NM" VARCHAR2(80), 
	"IS_UNDER" VARCHAR2(1) NOT NULL ENABLE, 
	"BD_MN_NM" NUMBER NOT NULL ENABLE, 
	"BD_SL_NO" NUMBER NOT NULL ENABLE, 
	"BD_NM" VARCHAR2(40), 
	"ZIP_NO" VARCHAR2(5), 
	"HJ_D_NM" VARCHAR2(40), 
	"LOC_X" NUMBER(15,6), 
	"LOC_Y" NUMBER(15,6), 
	 CONSTRAINT "PH_LOCATION_INFO" PRIMARY KEY ("ROAD_CD", "IS_UNDER", "BD_MN_NM", "BD_SL_NO", "BJ_D_CD")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 NOLOGGING 
  STORAGE(INITIAL 1048576 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS NOLOGGING
  STORAGE(INITIAL 1048576 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."SGG_CD" IS '시군구코드';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."BJ_D_CD" IS '법정동코드';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."SD_NM" IS '시도명';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."SGG_NM" IS '시군구명';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."EMD_NM" IS '읍면동명';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."ROAD_CD" IS '도로명코드';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."ROAD_NM" IS '도로명';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."IS_UNDER" IS '지하여부 [0 : 지상, 1 : 지하, 2 : 공중]';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."BD_MN_NM" IS '건물본번';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."BD_SL_NO" IS '건물부번';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."BD_NM" IS '건물명';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."ZIP_NO" IS '우편번호';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."HJ_D_NM" IS '관할행정동';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."LOC_X" IS 'X좌표';
   COMMENT ON COLUMN "TSPADM"."PH_LOCATION_INFO"."LOC_Y" IS 'Y좌표';
   COMMENT ON TABLE "TSPADM"."PH_LOCATION_INFO"  IS '위치정보';

