CREATE TABLE "TSPADM"."IF_CNCTOSM8" 
   (	"SVC_MGMT_NUM" VARCHAR2(30) NOT NULL ENABLE, 
	"REG_DY" VARCHAR2(8) NOT NULL ENABLE, 
	"ISUR_SSN" VARCHAR2(13), 
	"MOB_PHN_NO" VARCHAR2(20), 
	"MAKING_NO" VARCHAR2(20), 
	"MODEL_NO" VARCHAR2(20), 
	"AM_OUTCOME" NUMBER, 
	"AM_INQUERY" NUMBER, 
	"AM_FST_LIMIT" NUMBER, 
	"AM_TOTALCHARGE" NUMBER, 
	"AM_SELFCHARGE" NUMBER, 
	"AM_MAX" NUMBER, 
	"UKEY_USER_NM" VARCHAR2(30), 
	"ACDNT_PRC_USER_NM" VARCHAR2(13), 
	"REG_DT" DATE, 
	"REGR" VARCHAR2(10), 
	"UPD_DT" DATE, 
	"UPDR" VARCHAR2(10), 
	"PROD_FG" VARCHAR2(4), 
	"PROC_DT" VARCHAR2(10), 
	"USE_YN" VARCHAR2(4), 
	"ACCOUNT" VARCHAR2(100), 
	 CONSTRAINT "IF_CNCTOSM_PK8" PRIMARY KEY ("SVC_MGMT_NUM", "REG_DY")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOLOGGING 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

