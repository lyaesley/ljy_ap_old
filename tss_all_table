	"REGR" VARCHAR2(10), 
	"UPD_DT" DATE, 
	"UPDR" VARCHAR2(10), 
	"PROD_FG" VARCHAR2(4), 
	"PROC_DT" VARCHAR2(10), 
	"USE_YN" VARCHAR2(4), 
