   (	"SVC_NUM" RAW(2000) NOT NULL ENABLE, 
	"CTZ_CORP_NUM" RAW(2000) NOT NULL ENABLE, 
	"EQP_INSU_SER_NUM" NUMBER(10,0) NOT NULL ENABLE, 
