CREATE TABLE "TSPADM"."IF_ASTOSM8" 
   (	"SVC_MGMT_NUM" VARCHAR2(30) NOT NULL ENABLE, 
	"REG_DY" VARCHAR2(8) NOT NULL ENABLE, 
	"CONTR_NO" VARCHAR2(20), 
	"ACDNT_DY" VARCHAR2(14), 
	"ACDNT_DET" VARCHAR2(1000), 
