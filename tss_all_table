	"CANCEL_YN" VARCHAR2(1) DEFAULT 'N' NOT NULL ENABLE, 
	"MTCS_YN" VARCHAR2(1) DEFAULT 'N' NOT NULL ENABLE, 
	"PROD_ID" VARCHAR2(10), 
	"EQP_MDL_CD" VARCHAR2(5), 
	"ACDNT_DOC_CNT" VARCHAR2(2), 
	 CONSTRAINT "PK_TS_INSU_ACDNT" PRIMARY KEY ("CLAIM_ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOLOGGING 
  STORAGE(INITIAL 1048576 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS NOLOGGING
  STORAGE(INITIAL 1048576 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

