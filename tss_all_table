CREATE TABLE "TSPADM"."TS_TELINK_SMS#" 
   (	"CMP_MSG_ID" VARCHAR2(20) NOT NULL ENABLE, 
	"CMP_USR_ID" VARCHAR2(5) DEFAULT '00000', 
	"ODR_FG" CHAR(1) DEFAULT '1', 
	"SMS_GB" CHAR(1) DEFAULT '1', 
	"USED_CD" CHAR(3) DEFAULT '00', 
	"MSG_GB" CHAR(1) NOT NULL ENABLE, 
	"WRT_DTTM" CHAR(14), 
	"SND_DTTM" CHAR(14) NOT NULL ENABLE, 
	"SND_PHN_ID" RAW(2000), 
	"RCV_PHN_ID" RAW(2000) NOT NULL ENABLE, 
	"CALLBACK" VARCHAR2(15), 
	"SND_MSG" VARCHAR2(200) NOT NULL ENABLE, 
	"CMP_SND_DTTM" CHAR(14), 
	"CMP_RCV_DTTM" CHAR(14), 
	"REG_SND_DTTM" CHAR(14), 
	"REG_RCV_DTTM" CHAR(14), 
	"EXPIRE_VAL" NUMBER DEFAULT 0, 
	"SMS_ST" CHAR(1) DEFAULT 0, 
	"RSLT_VAL" NUMBER(4,0) DEFAULT 99, 
	"RSRVD_ID" VARCHAR2(20), 
	"RSRVD_WD" VARCHAR2(50), 
	 CONSTRAINT "TS_TELINK_SMS_PK" PRIMARY KEY ("CMP_MSG_ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOLOGGING 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."CMP_MSG_ID" IS 'SMS메세지ID(년월일시분초밀리초(TO_CHAR(SYSDATE,''YYYYMMDDHH24MISS'') || SUBSTR(TO_CHAR(DBMS_UTILITY.GET_TIME),2))';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."CMP_USR_ID" IS '서비스 ID';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."ODR_FG" IS '전송 우선 순위 구분';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."SMS_GB" IS '전송 위치 구분';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."USED_CD" IS '사용 구분';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."MSG_GB" IS '메시지 종류 구분(A.SMS , B.URL , C.해외SMS , D.해외URL)';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."WRT_DTTM" IS '메시지 작성시간';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."SND_DTTM" IS '메시지를 전송할 시간(즉시전송:현재시간, 예약발송:예약시간)';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."SND_PHN_ID" IS '보내는 사람 휴대폰 번호';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."RCV_PHN_ID" IS '받는 사람 휴대폰 번호';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."CALLBACK" IS '회신번호 (‘*’,’’,’번호’ 만 가능) KTF URL SMS전송일 경우 필수입력 사항임';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."SND_MSG" IS '메시지 내용';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."CMP_SND_DTTM" IS '업체에서 SKTelink로 전송한 시간';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."CMP_RCV_DTTM" IS '업체에서 결과를 통보 받은 시간';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."REG_SND_DTTM" IS 'SKTelink가 통신사로 메시지를 보낸 시간';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."REG_RCV_DTTM" IS 'SKTelink가 통신사에게  결과를 받은 시간';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."EXPIRE_VAL" IS '유효시간';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."SMS_ST" IS '메시지의 내부 처리 상태 (상태값 참조) 반드시 최초 입력 시 ‘0’으로 셋팅할 것';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."RSLT_VAL" IS '전송결과 (디폴트 값 99, 상태값 참조 )';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."RSRVD_ID" IS '분리 과금용 필드';
   COMMENT ON COLUMN "TSPADM"."TS_TELINK_SMS#"."RSRVD_WD" IS '최종 착신 이통사(SKT:S, KTF:K, LGT:L, NULL:N)';
   COMMENT ON TABLE "TSPADM"."TS_TELINK_SMS#"  IS 'SMS 발송 관리';

