CREATE TABLE "TSPADM"."PH_PROC_ACCOUNT" 
   (	"CLAIM_ID" VARCHAR2(12) NOT NULL ENABLE, 
	"SVC_MGMT_NUM" VARCHAR2(10) NOT NULL ENABLE, 
	"DEDUCTIBLE_PRI" NUMBER, 
	"DEVI_PRI_DIFF" NUMBER, 
	"USIM_PRI" NUMBER, 
	"SUM_PRI" NUMBER, 
	"CARD_MONEY" VARCHAR2(1), 
	"RESULT_YN" VARCHAR2(1), 
	"WAYBILL_NUM" VARCHAR2(30), 
	"REGR" VARCHAR2(20), 
	"UPDR" VARCHAR2(20), 
	"REG_DT" DATE, 
	"UPD_DT" DATE, 
	 CONSTRAINT "PH_PROC_ACCOUNT_PK" PRIMARY KEY ("CLAIM_ID", "SVC_MGMT_NUM")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOLOGGING 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "TSP_DATA" ;

   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."CLAIM_ID" IS '클레임ID';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."SVC_MGMT_NUM" IS '서비스관리번호';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."DEDUCTIBLE_PRI" IS '자기부담금';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."DEVI_PRI_DIFF" IS '기기차액';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."USIM_PRI" IS '유심비용';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."SUM_PRI" IS '총비용';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."CARD_MONEY" IS '카드결제/무통장입금';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."RESULT_YN" IS '결제완료여부';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."WAYBILL_NUM" IS '운송장번호';
   COMMENT ON COLUMN "TSPADM"."PH_PROC_ACCOUNT"."REG_DT" IS '입력일';
   COMMENT ON TABLE "TSPADM"."PH_PROC_ACCOUNT"  IS '자기부담금 결제관련히스토리';

CREATE TABLE "TSPADM"."PH_PROC_ACCOUNT_HST" 
   (	"CLAIM_ID" VARCHAR2(12) NOT NULL ENABLE, 
	"SVC_MGMT_NUM" VARCHAR2(10) NOT NULL ENABLE, 
	"DEDUCTIBLE_PRI" NUMBER, 
	"DEVI_PRI_DIFF" NUMBER, 
	"USIM_PRI" NUMBER, 
	"SUM_PRI" NUMBER, 
	"CARD_MONEY" VARCHAR2(1), 
	"WAYBILL_NUM" VARCHAR2(30), 
	"REGR" VARCHAR2(20), 
